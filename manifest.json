{
    "name": "Plus 50 - Twitch Auto Clicker",
    "version": "0.5",
    "description": "An extension to auto click on Twitch channel points",
    "permissions": ["https://*.twitch.tv/*"],
    "content_scripts": [
        {
          "matches": ["https://*.twitch.tv/*"],
            "js": [
            "clicker.js"
            ],
            "run_at": "document_idle"
        }
    ],
    "icons": 
    { "16": "/resources/icon16.png",
      "48": "/resources/icon48.png",
     "128": "/resources/icon128.png" },
    "manifest_version": 2
  }